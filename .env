# Конфигурация сервера
SERVER_PORT=8080
ENVIRONMENT=development

# Конфигурация MinIO
# ВАЖНО: Для Docker Compose используется имя сервиса 'minio', а не 'localhost'
MINIO_ENDPOINT=minio:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_BUCKET=university-schedules
MINIO_USE_SSL=false

# Для MinIO сервиса (совпадают с ACCESS_KEY и SECRET_KEY)
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin

# Конфигурация бакетов для обработки файлов
SOURCE_BUCKET=file-upload
TARGET_BUCKET=${MINIO_BUCKET}

# Паттерн пути к файлам в бакетах
FILE_PATH_PATTERN=universities/%s/courses/%s/types/%s/files/%s

# Кэширование
CACHE_TTL_MINUTES=10

# Presigned URLs
PRESIGNED_URL_TTL_MINUTES=15